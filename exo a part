AUTRE
    let age = prompt("Entrez votre age");
let isOk = confirm("etes vous sur(e)?");

if (age<2) {
    alert("BOIS BEBE")
}
else if (age>2 && age<=5) {
    alert("PLEUUUUUUUUUUUURETARACE")
}
else if (age>5 && age<=15) {
    alert("VAJOUERDANSTONCOIN")
}
else if (age>15 && age<=20) {
    alert("VACUVEY")
}
else if (age>20 && age<=60) {
    alert("CODE EN C CAR CEY COOL")
}
else if ((age>=60 && age <=65 ) || (age>=70 && age <=72)) {
    alert("AIE JAY MAL")
}

