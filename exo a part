AUTRE
function add(a, b) {
    return(a+b);
}
function sub(a, b) {
    return(a-b);
}
function mult(a, b) {
    return(a*b);
}
function div(a, b) {
    return(a/b);
}
let a= prompt("Que vaut a ?");
a=Number (a);

let b= prompt("Que vaut b ?");
b=Number (b);

console.log(add(a,b));
console.log(sub(a,b));
console.log(mult(a,b));
console.log(div(a,b));


